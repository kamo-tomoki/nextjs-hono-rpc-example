{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"a11y": {
				"noAccessKey": "error",
				"useHeadingContent": "error"
			},
			"correctness": {
				"noUndeclaredVariables": "error",
				"noUnusedVariables": "error"
			},
			"style": {
				"noNamespace": "error"
			}
		}
	}
}
